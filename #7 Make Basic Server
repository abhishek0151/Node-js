const http = require('http');
http.creatServer((req,resp){
resp.write("Hello World");
//resp.write("<h1>Hello World<h1>");
res.end();
}).listen(4500);

or,

const http = require('http');
function dataControl(req,resp)
{
  res.write("Hello");
  res.end();
}
http.createServer(dataControl).listen(4500);
